				<i class="icon-search-3"></i>
				<input type="hidden" name="sitesearch" value="misoftware.com.br" />
				<input type="search" name="q" />
			</form>
		</div>

		<div class="body-border">
			@if(ViewBag.FullLayout != null)
			{
				@RenderBody()
			}
			else
