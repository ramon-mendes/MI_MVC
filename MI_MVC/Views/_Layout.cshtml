									@*<li><img src="~/Content/img/arrow-e@2x.png" /><a href="/Home/Misc">Misc software</a></li>*@
								</ul>

								@if(Request.Cookies["MidiView"] != null)
								{
									<hr />

									<ul class="ul-main">
										<li><img src="~/Content/img/arrow-e@2x.png" /><a href="http://fontdrop.me/Download/Status">fontdrop status</a></li>
										<li><img src="~/Content/img/arrow-e@2x.png" /><a href="http://fontdrop.me/Home/Referers">fontdrop Referers</a></li>
										<li><img src="~/Content/img/arrow-e@2x.png" /><a href="/IOTCheguei">IOTCheguei</a></li>
