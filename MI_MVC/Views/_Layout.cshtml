<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<meta name="keywords" content="Sciter, C#, C++, TIScript, HTML, CSS, MONO, Windows, OSX, cross-platform, desktop, charts, WYSIWIG, forms, Bootstrap, WPF, WinForms" />
	<meta name="description" content="Software-house specialized in design of desktop and low-level software systems, mainly with Sciter technology for making modern HTML/CSS user interfaces." />
	<meta property="og:description" content="Software-house specialized in design of desktop and low-level software systems, mainly with Sciter technology for making modern HTML/CSS user interfaces." />

	<!-- Chrome, Firefox OS and Opera -->
	<meta name="theme-color" content="#2d4a8c">
	<!-- Windows Phone -->
	<meta name="msapplication-navbutton-color" content="#2d4a8c">
	<!-- iOS Safari -->
	<meta name="apple-mobile-web-app-status-bar-style" content="#2d4a8c">

	<title>
		@if(ViewData["Title"] != null)
		{
			@Html.Raw(ViewData["Title"].ToString() + " &middot; ");
		}
		MI Software
	</title>

	<link rel="alternate" type="application/rss+xml" title="RSS" href="/Home/RSS2" />

	<link rel="icon" href="~/Content/img/favicon-128.png" sizes="128x128">
	<link rel="icon" href="~/Content/img/favicon-32.png" sizes="32x32">
	<link rel="icon" href="~/Content/img/favicon-24.png" sizes="24x24">
	<link rel="icon" href="~/Content/img/favicon-16.png" sizes="16x16">

    @*<link href="~/Content/bootstrap-4-dev/scss/bootstrap-flex.min.css" rel="stylesheet" />*@
	<link href="~/Content/bootstrap-4-dev/scss/bootstrap.min.css" rel="stylesheet" />
	<link href="~/Content/site.min.css" rel="stylesheet" />
	<link href="~/Content/bundles/fontello/css/fontello.css" rel="stylesheet" />

	<link href="~/Content/fonts/nexa/stylesheet.css" rel="stylesheet" />
	<link href='https://fonts.googleapis.com/css?family=Days+One' rel='stylesheet' type='text/css' />
	<link href='https://fonts.googleapis.com/css?family=Open+Sans:400,300,300italic,600,600italic,700,700italic' rel='stylesheet' type='text/css' />

	@Scripts.Render("~/bundles/modernizr")
	@Scripts.Render("~/bundles/jquery")
	@RenderSection("head", required: false)
</head>

<body>
