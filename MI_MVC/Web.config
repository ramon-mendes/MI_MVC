  </runtime>
  <system.webServer>
    <!--
	Permite SciterStatus receber arquivos gigantes
	<security>
      <requestFiltering allowDoubleEscaping="true">
        <requestLimits maxAllowedContentLength="1000000000" />
      </requestFiltering>
    </security>-->
    <staticContent>
      <remove fileExtension=".eot" />
      <remove fileExtension=".ttf" />
      <remove fileExtension=".svg" />
      <remove fileExtension=".woff" />
      <remove fileExtension=".woff2" />
      <remove fileExtension=".otf" />
      <mimeMap fileExtension=".eot" mimeType="application/vnd.ms-fontobject" />
      <mimeMap fileExtension=".ttf" mimeType="application/octet-stream" />
      <mimeMap fileExtension=".svg" mimeType="image/svg+xml" />
      <mimeMap fileExtension=".woff" mimeType="application/font-woff" />
      <mimeMap fileExtension=".woff2" mimeType="application/font-woff2" />
      <mimeMap fileExtension=".otf" mimeType="application/font-otf" />
    </staticContent>
    <rewrite>
      <rules>
        <rule name="ADVOCACIA.RS">
          <match url="(.*)" />
          <conditions>
            <add input="{HTTP_HOST}" pattern="advocacia.rs" />
          </conditions>
          <action type="Rewrite" url="/mendesadv/{R:1}" />
        </rule>
        <rule name="ENGENHARIA.RS">
          <match url="(.*)" />
          <conditions>
            <add input="{HTTP_HOST}" pattern="engenharia.rs" />
          </conditions>
          <action type="Rewrite" url="/mendeseng/{R:1}" />
        </rule>
        <rule name="CHAMFERJS">
          <match url="(.*)" />
          <conditions>
            <add input="{HTTP_HOST}" pattern="chamferjs.misoftware.com.br" />
          </conditions>
          <action type="Rewrite" url="/Chamfer.js-master/{R:1}" />
        </rule>
        <!--<rule name="Redirect to non-www" stopProcessing="true">
          <match url="(.*)" negate="false"></match>
          <conditions>
            <add input="{HTTP_HOST}" pattern="^misoftware\.rs$" negate="true"></add>
          </conditions>
          <action type="Redirect" url="http://misoftware.com.br/{R:1}"></action>
        </rule>-->
      </rules>
    </rewrite>
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
      <remove name="TRACEVerbHandler" />
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
    </handlers>
  </system.webServer>
